<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ejb="http://java.sun.com/xml/ns/javaee" xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
	version="3.1">
	<enterprise-beans>
		<session>
			<ejb-name>MailService</ejb-name>
			<ejb-class>de.warehouse.mail.MailService</ejb-class>
			<env-entry>
				<env-entry-name>de.warehouse.mail.MailService/username</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>david.melcher@gmx.de</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>de.warehouse.mail.MailService/password</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>geheim$123</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>de.warehouse.mail.MailService/smtpServer</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>mail.gmx.net</env-entry-value>
			</env-entry>
		</session>
	</enterprise-beans>
</ejb-jar>